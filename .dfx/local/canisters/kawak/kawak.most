// Version: 1.0.0
type DraftEntry =
  {draftedAT : Int; id : Nat; owner : Text; text : Text; title : Text};
type EssayEntry =
  {
    aid : Principal;
    essayCost : Nat;
    id : Nat;
    owner : Text;
    reviewTimes : Nat32;
    reviewed : Bool;
    submittedAt : Int;
    text : Text;
    title : Text;
    topic : Text;
    wordCount : Nat
  };
actor {
  stable var stableAdmins : [Principal];
  stable var stableDrafts : [DraftEntry];
  stable var stableEssays : [EssayEntry]
};
